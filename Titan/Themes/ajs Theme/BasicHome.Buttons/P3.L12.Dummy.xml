<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls> 

   	<control Style="HomeButtonPictureDummy">
        <description>BasicHome DUMMY NoFocus</description>
        <type>image</type>
        <id>0</id>
        <posX>#L12left</posX>
        <posY>#L12top</posY>
        <animation effect="zoom" start="100,100" end="102,102" time="200" condition="control.hasfocus(450)">conditional</animation>
        <visible>!Control.IsVisible(60093)+control.IsVisible(10301)</visible>  
    </control>

  	<control Style="HomeButtonMenuDummy">
        <description>BasicHome DUMMY</description>
        <type>button</type>
        <id>450</id>
        <posX>#L12lefttext</posX>
        <posY>#L12toptext</posY>
        <onright>650</onright>
        <onleft>250</onleft>
        <onup>450</onup>
        <ondown>550</ondown>  
        <visible allowhiddenfocus="true">![Control.IsVisible(60093)|control.isvisible(90309)|control.hasfocus(99999)]+Control.IsVisible(10301)</visible>
    </control>
</controls>
</window>