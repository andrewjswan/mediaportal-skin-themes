<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls> 

   	<control Style="HomeButtonPictureDummy">
 	    <description>BasicHome DUMMY NoFocus</description>
 	    <type>image</type>
 	    <id>0</id>
  		<posX>#L13left</posX>
       	<posY>#L13top</posY>
       	<animation effect="zoom" start="100,100" end="102,102" time="200" condition="control.hasfocus(650)">conditional</animation>
 		<visible>control.IsVisible(10301)</visible>  
 	</control>

  	<control Style="HomeButtonMenuDummy">
    	<description>BasicHome DUMMY</description>
      	<type>button</type>
      	<id>650</id>
		<posX>#L13lefttext</posX>
	    <posY>#L13toptext</posY>
        <textYOff>#textYoff</textYOff>
		<onright>2</onright>
		<onleft>450</onleft>
      	<onup>650</onup>
		<ondown>750</ondown>  
  	    <visible allowhiddenfocus="true">![control.isvisible(90309)|control.hasfocus(99999)]+Control.IsVisible(10301)</visible>
	</control>
</controls>
</window>