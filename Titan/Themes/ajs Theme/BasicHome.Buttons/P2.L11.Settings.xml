<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
<controls> 

   	<control Style="HomeButtonPicture">
 	    <description>BasicHome Settings NoFocus</description>
 	    <type>image</type>
 	    <id>0</id>
  		<posX>#L11left</posX>
       	<posY>#L11top</posY>
 	    <texture>basichome_button_settings.png</texture>
       	<animation effect="zoom" start="100,100" end="102,102" time="200" condition="control.hasfocus(200)">conditional</animation>
 		<visible>control.IsVisible(10201)</visible>  
 	</control>

  	<control Style="HomeButtonMenu">
    	<description>BasicHome Settings</description>
      	<type>button</type>
      	<id>200</id>
		<posX>#L11lefttext</posX>
	    <posY>#L11toptext</posY>
        <hyperlink>4</hyperlink>
        <label>897</label>
  	    <onright>400</onright>
  	    <onup>200</onup>
  	    <onleft>6</onleft>
  	    <ondown>300</ondown>
  	    <visible allowhiddenfocus="true">![control.isvisible(90209)|control.hasfocus(99999)]+Control.IsVisible(10201)</visible>
	</control>
</controls>
</window>